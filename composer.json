{
    "name": "fsasvari/laravel-trailing-slash",
    "description": "Adds redirection with trailing slash in Laravel.",
    "keywords": ["redirect", "laravel", "php"],
    "license": "MIT",
    "homepage": "https://github.com/fsasvari/laravel-trailing-slash",
    "authors": [
        {
            "name": "Frano Sasvari",
            "email": "sasvari.frano@gmail.com"
        }
    ],
    "require": {
        "illuminate/routing": "~5.6.0|~5.7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTrailingSlash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTrailingSlash\\RoutingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}