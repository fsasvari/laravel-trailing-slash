{
    "name": "streetyo/laravel-trailing-slash",
    "description": "The package that adds redirection with trailing slash to Laravel framework.",
    "keywords": ["redirect", "laravel", "php"],
    "license": "MIT",
    "homepage": "https://github.com/fsasvari/laravel-trailing-slash",
    "authors": [
        {
            "name": "Frano Sasvari",
            "email": "sasvari.frano@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/routing": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.4.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelTrailingSlash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTrailingSlash\\RoutingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
